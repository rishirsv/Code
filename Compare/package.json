{
  "name": "@repo/compare",
  "version": "0.1.0",
  "private": true,
  "description": "Interactive CLI to synthesize GPT-5 custom instructions from rated prompt explorations",
  "type": "module",
  "bin": {
    "compare": "./dist/index.js"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --format esm --dts --sourcemap --out-dir dist",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest --runInBand"
  },
  "keywords": [
    "cli",
    "gpt-5",
    "custom-instructions",
    "openrouter"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "prompts": "^2.4.2",
    "undici": "^6.18.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  }
}
